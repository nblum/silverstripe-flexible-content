{
  "name": "nblum/silverstripe-flexible-content",
  "description": "Pages based on data objects for better content structure",
  "type": "silverstripe-module",
  "homepage": "",
  "keywords": [
    "silverstripe",
    "content"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nico Blum",
      "email": "mail@blum-nico.de"
    }
  ],
  "require": {
    "silverstripe/framework": "^3.0",
    "silverstripe-australia/gridfieldextensions": "^1.0",
    "heyday/silverstripe-versioneddataobjects": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Nblum\\FlexibleContent\\": "src/"
    }
  },
  "extra": {
    "installer-name": "flexiblecontent",
    "expose": [
      "css",
      "javascript"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "suggest": {
    "nblum/silverstripe-flexible-content-elements": "Provides a basic set of content elements"
  }
}
